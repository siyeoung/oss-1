      "source": [
        "oasis = [int(input()) for _ in range(int(input()))]\n",
        "\n",
        "stack = []\n",
        "result = 0\n",
        "\n",
        "for o in oasis:\n",
        "  while stack and stack[-1]<o: # >\n",
        "    stack.pop()\n",
        "    result += 1\n",
        "\n",
        "  stack.append(o)\n",
        "\n",
        "  if len(stack)>0:\n",
        "    result += len(stack)-1\n",
        "\n",
        "\n",
        "print(result)"
      ]
    }
  ]
}